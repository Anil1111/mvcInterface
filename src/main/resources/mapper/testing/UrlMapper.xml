<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nine.intface.testing.dao.UrlMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.nine.intface.testing.po.Url">
        <id column="id" property="id" />
        <result column="url" property="url" />
        <result column="protocol" property="protocol" />
        <result column="http_method" property="httpMethod" />
        <result column="url_name" property="urlName" />
        <result column="update_time" property="updateTime" />
        <result column="create_time" property="createTime" />
        <result column="remark" property="remark" />
        <result column="delete_flag" property="deleteFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url, protocol, http_method, url_name, update_time, create_time, remark, delete_flag
    </sql>

</mapper>
