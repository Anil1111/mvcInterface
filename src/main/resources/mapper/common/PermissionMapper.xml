<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nine.intface.common.dao.PermissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.nine.intface.common.po.Permission">
        <id column="id" property="id" />
        <result column="permission_name" property="permissionName" />
        <result column="describtion" property="describtion" />
        <result column="update_time" property="updateTime" />
        <result column="create_time" property="createTime" />
        <result column="delete_flag" property="deleteFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, permission_name, describtion, update_time, create_time, delete_flag
    </sql>

</mapper>
